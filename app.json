{
  "expo": {
    "name": "Power Route EV",
    "slug": "power-route-ev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-PREV.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/PowerRouteEV.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A7EA4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.power.route"
    },
    "android": {
      "package": "com.power.route",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-PREV.png",
        "backgroundColor": "#0A7EA4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
